{"ast":null,"code":"//import MultiStepForm from \"@/components/MultiStepForm.vue\";\nimport axios from 'axios';\nexport default {\n  name: 'ApplicationPage',\n  components: {\n    // MultiStepForm,\n    // ... (other components)\n  },\n  data() {\n    return {\n      selectedSection: 'GramPanchayat',\n      // Initialize with Gram Panchayat as the default section\n      applications: [],\n      // Initialize an empty array to store applications\n      gramPanchayatOprator: false,\n      gramPanchayt: false,\n      panchaytSamitiOprator: false,\n      panchaytSamiti: false,\n      zillaParishad: false,\n      user: localStorage.getItem('login') ? JSON.parse(localStorage.getItem('login')) : alert('User not logged in.')\n    };\n  },\n  computed: {\n    // Create computed properties for each table based on the selected section\n    gramPanchayatApplications() {\n      return this.applications.filter(application => !application.dtGramPanchayatAuthDate);\n    },\n    panchayatSamitiApplications() {\n      return this.applications.filter(application => application.dtGramPanchayatAuthDate && !application.dtPanchayatSamitiAuthDate);\n    },\n    zillaParishadApplications() {\n      return this.applications.filter(application => application.dtPanchayatSamitiAuthDate);\n    },\n    selectedOptionComponent() {\n      return this.selectedOption === 'new' ? 'MultiStepForm' : null;\n    }\n  },\n  methods: {\n    selectSection(section) {\n      this.selectedSection = section;\n    },\n    authorizeApplication(applicationId, authority) {\n      const jsonData = {\n        id: applicationId\n      };\n      axios.post(`http://127.0.0.1:5555/authorize${authority}`, jsonData, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(response => {\n        // Handle successful authorization\n        // Update your frontend accordingly\n        const authorizationDate = response.data.dtGramPanchayatAuthDate;\n        console.log(authorizationDate);\n        this.fetchApplications();\n      }).catch(error => {\n        // Handle error\n        console.error('Error authorizing application:', error);\n      });\n    },\n    rejectApplication(applicationId, authority) {\n      const jsonData = {\n        id: applicationId\n      };\n      axios.post(`http://127.0.0.1:5555/reject${authority}`, jsonData, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(response => {\n        // Handle successful rejection\n        // Update your frontend accordingly\n        const authorizationDate = response.data.dtGramPanchayatAuthDate;\n        console.log(authorizationDate);\n        this.fetchApplications();\n      }).catch(error => {\n        // Handle error\n        console.error('Error rejecting application:', error);\n      });\n    },\n    selectOption(option) {\n      this.selectedOption = option;\n    },\n    reloadApplicationData() {\n      this.fetchApplications();\n    },\n    deleteApplication(applicationId) {\n      const jsonData = {\n        id: applicationId\n      };\n      axios.post('http://127.0.0.1:5555/deleteVihirInfo', jsonData, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(response => {\n        const deletedApplicationId = response.data.id;\n        console.log(`Application with ID ${deletedApplicationId} has been deleted.`);\n\n        // Remove the deleted application from the frontend\n        this.applications = this.applications.filter(application => application.id !== applicationId);\n        this.reloadApplicationData();\n      }).catch(error => {\n        console.error('Error deleting application:', error);\n      });\n    },\n    fetchApplications() {\n      axios.get('http://127.0.0.1:5555/GetVihirInfo').then(applications => {\n        console.log(applications);\n        this.applications = applications.data;\n      }).catch(error => console.error('Error:', error));\n    },\n    editApplication(applicationId) {\n      this.selectedOption = 'new'; // Set the selectedOption to 'new'\n      this.selectedApplicationId = applicationId; // Store the selected application ID\n    },\n\n    goBackFromChild() {\n      this.selectedOption = null;\n      this.reloadApplicationData();\n    }\n  },\n  mounted() {\n    this.gramPanchayatOprator = this.user['gramPanchayatOprator'] !== undefined ? this.user.gramPanchayatOprator : false;\n    this.gramPanchayt = this.user['gramPanchayt'] !== undefined ? this.user.gramPanchayt : false;\n    this.panchaytSamitiOprator = this.user['panchaytSamitiOprator'] !== undefined ? this.user.panchaytSamitiOprator : false;\n    this.panchaytSamiti = this.user['panchaytSamiti'] !== undefined ? this.user.panchaytSamiti : false;\n    this.zillaParishad = this.user['zillaParishad'] !== undefined ? this.user.zillaParishad : false;\n    console.log(this.user.id);\n    this.fetchApplications(); // Fetch applications when the component is mounted\n  }\n};","map":{"version":3,"names":["axios","name","components","data","selectedSection","applications","gramPanchayatOprator","gramPanchayt","panchaytSamitiOprator","panchaytSamiti","zillaParishad","user","localStorage","getItem","JSON","parse","alert","computed","gramPanchayatApplications","filter","application","dtGramPanchayatAuthDate","panchayatSamitiApplications","dtPanchayatSamitiAuthDate","zillaParishadApplications","selectedOptionComponent","selectedOption","methods","selectSection","section","authorizeApplication","applicationId","authority","jsonData","id","post","headers","then","response","authorizationDate","console","log","fetchApplications","catch","error","rejectApplication","selectOption","option","reloadApplicationData","deleteApplication","deletedApplicationId","get","editApplication","selectedApplicationId","goBackFromChild","mounted","undefined"],"sources":["C:\\xampp\\htdocs\\PSPM\\Final-Project\\src\\components\\ApplicationPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"application\">\r\n    <div class=\"application-options\">\r\n      <!-- Add buttons to switch between sections -->\r\n      <button v-if=\"gramPanchayatOprator\" class=\"btn btn-info mb-4 text-white me-1\">Gram Panchayat Oprator</button>\r\n      <button v-if=\"gramPanchayt\" class=\"btn btn-info mb-4 text-white me-1\">Gram Panchayat</button>\r\n      <button v-if=\"panchaytSamitiOprator\" class=\"btn btn-info mb-4 text-white me-1\">Panchayat Samiti Oprator</button>\r\n      <button v-if=\"panchaytSamiti\" class=\"btn btn-info mb-4 text-white me-1\">Panchayat Samiti</button>\r\n      <button v-if=\"zillaParishad\" class=\"btn btn-info mb-4 text-white me-1\">Zilla Parishad</button>\r\n\r\n      <!-- Display the selected table based on the section -->\r\n      <div >\r\n        <table class=\"table table-striped\">\r\n          <!-- Table headers for Gram Panchayat -->\r\n          <thead>\r\n            <tr>\r\n              <th>#</th>\r\n              <th>Date of Creation</th>\r\n              <th>First Name</th>\r\n              <th>Last Name</th>\r\n              <th>Form Number</th>\r\n              <th>Gram Panchayat Authority</th>\r\n              <th>Action</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"(application, index) in gramPanchayatApplications\" :key=\"application.id\">\r\n              <!-- Display Gram Panchayat applications -->\r\n              <td>{{ ++index }}</td>\r\n              <td>{{ application.dtDateOfCreation }}</td>\r\n              <td>{{ application.ncharFirstName }}</td>\r\n              <td>{{ application.ncharLastName }}</td>\r\n              <td>{{ application.ncharFormNumber }}</td>\r\n              <td>\r\n                <!-- Check if dtGramPanchayatAuthDate is present and ynGramPanchayatAuthorized is true -->\r\n                <template v-if=\"application.dtGramPanchayatAuthDate && application.ynGramPanchayatAuthorized\">\r\n                  <!-- Show the result from ynGramPanchayatAuthorized -->\r\n                  {{ application.ynGramPanchayatAuthorized }}\r\n                </template>\r\n                <!-- If dtGramPanchayatAuthDate is not present or ynGramPanchayatAuthorized is false, show buttons -->\r\n                <template v-else>\r\n                  <button class=\"btn btn-sm rounded btn-success me-2\" @click=\"authorizeApplication(application.id, 'GramPanchayat')\" v-if=\"!application.dtGramPanchayatAuthDate\">\r\n                    Authorize\r\n                  </button>\r\n                  <button class=\"btn btn-sm rounded btn-danger\" @click=\"rejectApplication(application.id, 'GramPanchayat')\" v-if=\"!application.dtGramPanchayatAuthDate\">\r\n                    Reject\r\n                  </button>\r\n                </template>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n//import MultiStepForm from \"@/components/MultiStepForm.vue\";\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'ApplicationPage',\r\n  components: {\r\n   // MultiStepForm,\r\n    // ... (other components)\r\n  },\r\n  data() {\r\n    return {\r\n      selectedSection: 'GramPanchayat', // Initialize with Gram Panchayat as the default section\r\n      applications: [], // Initialize an empty array to store applications\r\n      gramPanchayatOprator  : false,\r\n      gramPanchayt          : false,\r\n      panchaytSamitiOprator : false,\r\n      panchaytSamiti        : false,\r\n      zillaParishad         : false,\r\n      user       : (localStorage.getItem('login')) ? JSON.parse(localStorage.getItem('login')) : alert('User not logged in.'),\r\n    };\r\n  },\r\n  computed: {\r\n    // Create computed properties for each table based on the selected section\r\n    gramPanchayatApplications() {\r\n      return this.applications.filter(application => !application.dtGramPanchayatAuthDate);\r\n    },\r\n    panchayatSamitiApplications() {\r\n  return this.applications.filter(application => application.dtGramPanchayatAuthDate && !application.dtPanchayatSamitiAuthDate);\r\n},\r\n    zillaParishadApplications() {\r\n      return this.applications.filter(application => application.dtPanchayatSamitiAuthDate);\r\n    },\r\n    selectedOptionComponent() {\r\n      return this.selectedOption === 'new' ? 'MultiStepForm' : null;\r\n    }\r\n  },\r\n  methods: {\r\n    selectSection(section) {\r\n      this.selectedSection = section;\r\n    },\r\n    authorizeApplication(applicationId, authority) {\r\n    const jsonData = {\r\n      id: applicationId,\r\n    };\r\n\r\n    axios\r\n      .post(`http://127.0.0.1:5555/authorize${authority}`, jsonData, {\r\n        headers: { 'Content-Type': 'application/json' },\r\n      })\r\n      .then((response) => {\r\n        // Handle successful authorization\r\n        // Update your frontend accordingly\r\n        const authorizationDate = response.data.dtGramPanchayatAuthDate;\r\n        console.log(authorizationDate)\r\n        this.fetchApplications();\r\n      })\r\n      .catch((error) => {\r\n        // Handle error\r\n        console.error('Error authorizing application:', error);\r\n      });\r\n  },\r\n  rejectApplication(applicationId, authority) {\r\n    const jsonData = {\r\n      id: applicationId,\r\n    };\r\n\r\n    axios\r\n      .post(`http://127.0.0.1:5555/reject${authority}`, jsonData, {\r\n        headers: { 'Content-Type': 'application/json' },\r\n      })\r\n      .then((response) => {\r\n        // Handle successful rejection\r\n        // Update your frontend accordingly\r\n        const authorizationDate = response.data.dtGramPanchayatAuthDate;\r\n        console.log(authorizationDate)\r\n        this.fetchApplications();\r\n      })\r\n      .catch((error) => {\r\n        // Handle error\r\n        console.error('Error rejecting application:', error);\r\n      });\r\n  },\r\n    selectOption(option) {\r\n      this.selectedOption = option;\r\n    },\r\n    reloadApplicationData() {\r\n      this.fetchApplications();\r\n    },\r\n    deleteApplication(applicationId) {\r\n      const jsonData = { id: applicationId };\r\n\r\n      axios.post('http://127.0.0.1:5555/deleteVihirInfo', jsonData, { headers: { 'Content-Type': 'application/json' } })\r\n      .then(response =>\r\n        {\r\n          const deletedApplicationId = response.data.id;\r\n          console.log(`Application with ID ${deletedApplicationId} has been deleted.`);\r\n\r\n          // Remove the deleted application from the frontend\r\n          this.applications = this.applications.filter(application => application.id !== applicationId);\r\n          this.reloadApplicationData();\r\n        })\r\n        .catch(error => {\r\n          \r\n          console.error('Error deleting application:', error);\r\n        });\r\n    },\r\n    fetchApplications()\r\n    {\r\n      axios.get('http://127.0.0.1:5555/GetVihirInfo')\r\n      .then(applications =>\r\n      {\r\n        console.log(applications)\r\n         this.applications = applications.data\r\n      })\r\n      .catch(error => console.error('Error:', error));\r\n    },\r\n    editApplication(applicationId) {\r\n      this.selectedOption = 'new'; // Set the selectedOption to 'new'\r\n      this.selectedApplicationId = applicationId; // Store the selected application ID\r\n    },\r\n    goBackFromChild() {\r\n      this.selectedOption = null;\r\n      this.reloadApplicationData();\r\n    }\r\n  },\r\n  mounted()\r\n  {\r\n    this.gramPanchayatOprator = (this.user['gramPanchayatOprator'] !== undefined) ? this.user.gramPanchayatOprator : false;\r\n    this.gramPanchayt = (this.user['gramPanchayt'] !== undefined) ? this.user.gramPanchayt : false;\r\n    this.panchaytSamitiOprator = (this.user['panchaytSamitiOprator'] !== undefined) ? this.user.panchaytSamitiOprator : false;\r\n    this.panchaytSamiti = (this.user['panchaytSamiti'] !== undefined) ? this.user.panchaytSamiti : false;\r\n    this.zillaParishad = (this.user['zillaParishad'] !== undefined) ? this.user.zillaParishad : false;\r\n    console.log(this.user.id);\r\n    this.fetchApplications(); // Fetch applications when the component is mounted\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.application {\r\n  padding: 40px;\r\n  background-color: #ffffff;\r\n  box-shadow: 0 0 2px var(--grey-color-light);\r\n}\r\n\r\n.application-options {\r\n  /* Add your custom styles for the application options container */\r\n}\r\n</style>\r\n"],"mappings":"AA0DA;AACA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACX;IACC;EAAA,CACD;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE,eAAe;MAAE;MAClCC,YAAY,EAAE,EAAE;MAAE;MAClBC,oBAAmB,EAAK,KAAK;MAC7BC,YAAW,EAAa,KAAK;MAC7BC,qBAAoB,EAAI,KAAK;MAC7BC,cAAa,EAAW,KAAK;MAC7BC,aAAY,EAAY,KAAK;MAC7BC,IAAG,EAAWC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC,GAAIC,IAAI,CAACC,KAAK,CAACH,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC,IAAIG,KAAK,CAAC,qBAAqB;IACxH,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR;IACAC,yBAAyBA,CAAA,EAAG;MAC1B,OAAO,IAAI,CAACb,YAAY,CAACc,MAAM,CAACC,WAAU,IAAK,CAACA,WAAW,CAACC,uBAAuB,CAAC;IACtF,CAAC;IACDC,2BAA2BA,CAAA,EAAG;MAChC,OAAO,IAAI,CAACjB,YAAY,CAACc,MAAM,CAACC,WAAU,IAAKA,WAAW,CAACC,uBAAsB,IAAK,CAACD,WAAW,CAACG,yBAAyB,CAAC;IAC/H,CAAC;IACGC,yBAAyBA,CAAA,EAAG;MAC1B,OAAO,IAAI,CAACnB,YAAY,CAACc,MAAM,CAACC,WAAU,IAAKA,WAAW,CAACG,yBAAyB,CAAC;IACvF,CAAC;IACDE,uBAAuBA,CAAA,EAAG;MACxB,OAAO,IAAI,CAACC,cAAa,KAAM,KAAI,GAAI,eAAc,GAAI,IAAI;IAC/D;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAACC,OAAO,EAAE;MACrB,IAAI,CAACzB,eAAc,GAAIyB,OAAO;IAChC,CAAC;IACDC,oBAAoBA,CAACC,aAAa,EAAEC,SAAS,EAAE;MAC/C,MAAMC,QAAO,GAAI;QACfC,EAAE,EAAEH;MACN,CAAC;MAED/B,KAAI,CACDmC,IAAI,CAAE,kCAAiCH,SAAU,EAAC,EAAEC,QAAQ,EAAE;QAC7DG,OAAO,EAAE;UAAE,cAAc,EAAE;QAAmB;MAChD,CAAC,EACAC,IAAI,CAAEC,QAAQ,IAAK;QAClB;QACA;QACA,MAAMC,iBAAgB,GAAID,QAAQ,CAACnC,IAAI,CAACkB,uBAAuB;QAC/DmB,OAAO,CAACC,GAAG,CAACF,iBAAiB;QAC7B,IAAI,CAACG,iBAAiB,CAAC,CAAC;MAC1B,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QAChB;QACAJ,OAAO,